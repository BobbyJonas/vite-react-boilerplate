{
  "name": "vite-react-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "serve": "npm run dev",
    "dev": "vite --mode development",
    "build": "vite build",
    "build:beta": "vite build --mode beta",
    "build:release": "vite build --mode release",
    "build:legacy": "vite build --mode legacy ",
    "build:deploy": "vite build --base=/vite-react-ts/",
    "preview": "vite preview",
    "prebuild": "rimraf dist",
    "deploy": "gh-pages -d dist",
    "commitlint": "commitlint --from=master",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern --quiet --fix ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "stylelint-check": "stylelint-config-prettier-check",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,json}": [
      "npm run lint:fix",
      "git add --force"
    ],
    "**/*.{less}": [
      "npm run lint:style",
      "git add --force"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.8.1",
    "@ks/ks-watermark": "^1.0.8",
    "@monaco-editor/react": "^4.6.0",
    "@types/react-beautiful-dnd": "13.1.4",
    "ahooks": "^3.7.8",
    "antd": "^4.24.14",
    "axios": "^1.6.1",
    "jotai": "^2.5.1",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "moment": "^2.29.4",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dnd": "^15.1.2",
    "react-dnd-html5-backend": "^15.1.3",
    "react-dom": "^18.2.0",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0",
    "vite-plugin-ejs": "^1.7.0",
    "vite-raw-plugin": "^1.0.2",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@types/qs": "^6.9.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "classnames": "^2.3.2",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.31",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.9.2",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-less": "^2.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-compression": "^0.3.1",
    "vite-plugin-mock": "^2.9.8"
  }
}
